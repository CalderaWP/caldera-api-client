{
  "name": "@caldera-labs/api-client",
  "version": "0.1.0",
  "description": "API client for Caldera Forms and Caldera Forms Pro",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:pre && npm run build:compile && npm run build:post",
    "build:pre": "npm run test:once",
    "build:compile": "./node_modules/.bin/babel -d dist src/",
    "build:post": "npm run compress",
    "compress": "uglifyjs dist/index.js -c -m --source-map -o dist/index.min.js",
    "test": "jest --watch",
    "test:once": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run test && npm run build",
    "document": "./node_modules/.bin/esdoc",
    "lint": "eslint src/**, __tests__/**",
    "lint:watch": "esw src/**, __tests__/** --fix --watch",
    "lint:fix": "eslint src/**, __tests__/** --fix ",
    "flow:watch": "flow-watch",
    "flow": "flow",
    "wp-tests": "1",
    "wp-tests:install": "bash ./cf-browser-tests/install.sh",
    "wp-tests:start": "docker compose up -d",
    "wp-tests:stop": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calderawp/caldera-api-client.git"
  },
  "keywords": [
    "es6",
    "boilerplate",
    "npm",
    "module",
    "npm-module"
  ],
  "author": "Josh Pollock <Josh@calderawp.com>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/calderawp/caldera-api-client/issues"
  },
  "homepage": "http://calderalabs.org/caldera-api-client/",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.1",
    "esdoc": "^1.1.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest-cli": "^20.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-watch": "^3.1.5",
    "flow-bin": "^0.74.0",
    "flow-watch": "^1.1.3",
    "jest": "^20.0.4",
    "jest-fetch-mock": "^1.6.3",
    "locutus": "^2.0.9",
    "uglify-js": "^3.4.0"
  },
  "standard": {
    "plugins": [
      "flowtype"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  }
}
